<StructureDefinition xmlns="http://hl7.org/fhir">
    <meta>
        <tag>
            <system value="http://dsf.dev/fhir/CodeSystem/read-access-tag" />
            <code value="ALL" />
        </tag>
    </meta>
    <url value="http://eyematics.org/fhir/StructureDefinition/eyematics-receive-process-structure-definition" />
    <!-- version managed by bpe -->
    <version value="#{version}" />
    <name value="EyeMatics Receive Process Structure Definition" />
    <!-- status managed by bpe -->
    <status value="unknown" />
    <experimental value="false" />
    <!-- date managed by bpe -->
    <date value="#{date}" />
    <fhirVersion value="4.0.1" />
    <kind value="resource" />
    <abstract value="false" />
    <type value="Task" />
    <baseDefinition value="http://dsf.dev/fhir/StructureDefinition/task-base" />
    <derivation value="constraint" />
    <differential>
        <element id="Task.instantiatesCanonical">
            <path value="Task.instantiatesCanonical" />
            <fixedCanonical value="http://eyematics.org/bpe/Process/eyematicsReceiveProcess|#{version}" />
        </element>
        <!-- requester / recipient -->
        <element id="Task.input">
            <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-explicit-type-name">
                <valueString value="Parameter" />
            </extension>
            <path value="Task.input" />
            <min value="4" />
            <max value="4" />
        </element>
        <element id="Task.input:message-name">
            <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-explicit-type-name">
                <valueString value="Parameter" />
            </extension>
            <path value="Task.input" />
            <sliceName value="message-name" />
        </element>
        <element id="Task.input:message-name.value[x]">
            <path value="Task.input.value[x]" />
            <fixedString value="receiveProcessStartEventMessage" />
        </element>
        <element id="Task.input:business-key">
            <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-explicit-type-name">
                <valueString value="Parameter" />
            </extension>
            <path value="Task.input" />
            <sliceName value="business-key" />
            <max value="1" />
        </element>
        <element id="Task.input:correlation-key">
            <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-explicit-type-name">
                <valueString value="Parameter" />
            </extension>
            <path value="Task.input" />
            <sliceName value="correlation-key" />
            <max value="1" />
        </element>
        <!-- INPUT-DATA: CodeSystem and ValueSet have to be created. START -->
        <element id="Task.input:data-set-reference-input">
            <path value="Task.input" />
            <sliceName value="data-set-reference-input" />
            <min value="1" />
            <!-- <max value="99" /> -->
        </element>
        <element id="Task.input:data-set-reference-input.type">
            <path value="Task.input.type" />
            <binding>
                <strength value="required" />
                <valueSet value="http://eyematics.org/fhir/ValueSet/eyematics-receive-process-value-set" />
            </binding>
        </element>
        <element id="Task.input:data-set-reference-input.type.coding">
            <path value="Task.input.type.coding" />
                <min value="1" />
                <max value="1" />
        </element>
        <element id="Task.input:data-set-reference-input.type.coding.system">
            <path value="Task.input.type.coding.system" />
            <min value="1" />
            <fixedUri value="http://eyematics.org/fhir/CodeSystem/eyematics-receive-process-code-system" />
        </element>
        <element id="Task.input:data-set-reference-input.type.coding.code">
            <path value="Task.input.type.coding.code" />
            <min value="1" />
            <fixedCode value="data-set-reference-input" />
        </element>
        <element id="Task.input:data-set-reference-input.value[x]">
            <path value="Task.input.value[x]" />
            <type>
                <code value="Reference" />
            </type>
        </element>
        <!-- INPUT-DATA: CodeSystem and ValueSet have to be created. END -->
    </differential>
</StructureDefinition>