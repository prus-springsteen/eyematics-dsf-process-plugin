<CodeSystem xmlns="http://hl7.org/fhir">
    <meta>
        <tag>
            <system value="http://dsf.dev/fhir/CodeSystem/read-access-tag" />
            <code value="ALL" />
        </tag>
    </meta>
    <url value="http://eyematics.org/fhir/CodeSystem/eyematics-generic-process-data-set-status-codes-code-system" />
    <!-- version managed by bpe -->
    <version value="#{version}" />
    <name value="EyeMatics Generic Status" />
    <title value="EyeMatics Generic Status" />
    <!-- status managed by bpe -->
    <status value="unknown" />
    <experimental value="false" />
    <!-- date managed by bpe -->
    <date value="#{date}" />
    <publisher value="EyeMatics.org" />
    <description value="CodeSystem with standard values for the status of transferring data-sets" />
    <caseSensitive value="true" />
    <hierarchyMeaning value="grouped-by" />
    <versionNeeded value="false" />
    <content value="complete" />
    <!-- PROVIDE PROCESS: START -->
    <concept>
        <code value="data-read-failed" />
        <display value="Data Read Failed" />
        <definition value="Reading data from local FHIR-Repository failed." />
    </concept>
    <concept>
        <code value="data-bundle-failed" />
        <display value="Data Bundle Failed" />
        <definition value="Bundling data failed." />
    </concept>
    <concept>
        <code value="data-encrypt-failed" />
        <display value="Data Encrypt Failed" />
        <definition value="Failed to encrypt bundled data." />
    </concept>
    <concept>
        <code value="data-store-failed" />
        <display value="Data Store Failed" />
        <definition value="Storing data to provide failed." />
    </concept>
    <concept>
        <code value="data-provide-failed" />
        <display value="Date Provide Failed" />
        <definition value="Data could not be provided as receiving DIC is not reachable." />
    </concept>
    <concept>
        <code value="data-provide-forbidden" />
        <display value="Data Provide Forbidden" />
        <definition value="Forbidden to provide data to DIC." />
    </concept>
    <concept>
        <code value="data-receipt-missing" />
        <display value="Data Receipt Missing" />
        <definition value="Failed receiving data-set from DIC" />
    </concept>
    <!-- PROVIDE PROCESS: END -->
    <!-- RECEIVE PROCESS: START -->
    <concept>
        <code value="data-download-failed" />
        <display value="Data Download Failed" />
        <definition value="Data could not be downloaded from DIC." />
    </concept>
    <concept>
        <code value="data-decrypt-failed" />
        <display value="Data Decrypt Failed" />
        <definition value="Failed to decrypt downloaded data." />
    </concept>
    <concept>
        <code value="data-receive-successful" />
        <display value="Data Receive Successful" />
        <definition value="Received data successfully." />
    </concept>
    <concept>
        <code value="data-receipt-failed" />
        <display value="Data Decrypt Failed" />
        <definition value="Failed to send receipt to providing DIC." />
    </concept>
    <!-- RECEIVE PROCESS: END -->
</CodeSystem>